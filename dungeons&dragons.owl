     -->

    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasAlignment -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasAlignment">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isAlignmentOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Alignment"/>
        <dragonsontology:definition>Defines relationship between Character and Alignment. Each character has exactly one alignment</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasAlignment"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Defines relationship between Character and Alignment. Each character has exactly one alignment</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasBackground -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasBackground">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isBackroundOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Background"/>
        <dragonsontology:definition>Defines the relationship between Character and Background. Each character has only one background</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasBackground"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Defines the relationship between Character and Background. Each character has only one background</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasClass -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasClass">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isClassOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#CharacterClass"/>
        <dragonsontology:definition>Describes the relationship between Character and Class. Characters can have multiple classes, given that multiclassing is allowed as it is an optional feature. &quot;Multiclassing allows you to gain levels in multiple classes. Doing so lets you mix the abilities of those classes to realize a character concept that might not be reflected in one of the standard class options.&quot;</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasClass"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Describes the relationship between Character and Class. Characters can have multiple classes, given that multiclassing is allowed as it is an optional feature. &quot;Multiclassing allows you to gain levels in multiple classes. Doing so lets you mix the abilities of those classes to realize a character concept that might not be reflected in one of the standard class options.&quot;</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasProficiencyWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasProficiencyWith">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isProficiencyOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#CharacterProperty"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Item"/>
        <dragonsontology:definition>Defines the proficiency of a character with an item. Items are either weapons or armour. &quot;Your race, class, and feats can grant you proficiency with certain weapons or categories of weapons.&quot; Also, some classes are proficient with certain types of armour, whereas others do not wear armour.</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasProficiencyWith"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Defines the proficiency of a character with an item. Items are either weapons or armour. &quot;Your race, class, and feats can grant you proficiency with certain weapons or categories of weapons.&quot; Also, some classes are proficient with certain types of armour, whereas others do not wear armour.</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasRace -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasRace">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isRaceOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Race"/>
        <dragonsontology:definition>Describes the relationship between Character and Race. Each character has exactly one race. Any mixed races are considered separate races of their own (e.g. half-elf or half-orc) and the half that is not specified is always human.</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#hasRace"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Describes the relationship between Character and Race. Each character has exactly one race. Any mixed races are considered separate races of their own (e.g. half-elf or half-orc) and the half that is not specified is always human.</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isAlignmentOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isAlignmentOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Alignment"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <dragonsontology:definition>Describes the relationship between Alignment and Character. There are 9 different alignments for each character to choose from and each character must have one alignment but multiple characters can have the same alignment.</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isAlignmentOf"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Describes the relationship between Alignment and Character. There are 9 different alignments for each character to choose from and each character must have one alignment but multiple characters can have the same alignment.</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isBackroundOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isBackroundOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Background"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <dragonsontology:definition>Defines the relationship between Background and Character. Multiple characters can have the same background and there are 13 possible backgrounds</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isBackroundOf"/>
