        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Race"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <dragonsontology:definition>Defines the relationship between Race and Character. Multiple characters can have the same race and there are 9 main races, some of which have subraces from which to choose.</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isRaceOf"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
