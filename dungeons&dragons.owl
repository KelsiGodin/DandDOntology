        <owl:annotatedTarget>Defines the relationship between Background and Character. Multiple characters can have the same background and there are 13 possible backgrounds</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isClassOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isClassOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#CharacterClass"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Character"/>
        <dragonsontology:definition>Defines the relationship between Character Class and Character. Multiple characters can have the same class and there are 12 possible classes</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isClassOf"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Defines the relationship between Character Class and Character. Multiple characters can have the same class and there are 12 possible classes</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isProficiencyOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isProficiencyOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#Item"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#CharacterProperty"/>
        <dragonsontology:definition>Describes the relationship between Item and Character Property. Multiple character properties (race, character class) can give a character proficiency with a particular item (armour, weapon)</dragonsontology:definition>
    </owl:ObjectProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isProficiencyOf"/>
        <owl:annotatedProperty rdf:resource="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition"/>
        <owl:annotatedTarget>Describes the relationship between Item and Character Property. Multiple character properties (race, character class) can give a character proficiency with a particular item (armour, weapon)</owl:annotatedTarget>
        <dragonsontology:source>ISBN-10: 0786965606
ISBN-13: 978-0786965601</dragonsontology:source>
    </owl:Axiom>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isRaceOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#isRaceOf">
