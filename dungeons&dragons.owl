<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/user/ontologies/2019/1/untitled-ontology-6#"
     xml:base="http://www.semanticweb.org/user/ontologies/2019/1/untitled-ontology-6"
     xmlns:dragonsontology="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology">
        <license>This ontology is under the CC-BY-NC license.</license>
        <source>ISBN-10: 0786965606
ISBN-13: 978-0786965601

The same content can also be found at http://orc-news.ru/dnd5eng.pdf</source>
        <rdfs:comment>A Dungeons and Dragons ontology that describes characters based various character properties and items, based on the 5th edition rules.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#abilityScoreIncrease -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#abilityScoreIncrease">
        <dragonsontology:definition>The amount an ability goes up depending on race and subrace chosen by the player. Abilities include Constitution, Strength, Dexterity, Wisdom, Intelligence, and Charisma.</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#armourClass -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#armourClass">
        <dragonsontology:definition>&quot;Your Armor Class (AC) represents how well your character avoids being wounded in battle. Things that contribute to your AC include the armor you wear, the shield you carry, and your Dexterity modifier.&quot;</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#cost -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#cost">
        <dragonsontology:definition>The amount of money an item costs to buy. It is measured in gold pieces (gp), silver pieces (sp), and copper pieces (cp). One gold piece is equal to 10 silver pieces and 100 copper pieces.</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#damage -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#damage">
        <dragonsontology:definition>The number and type of dice that a character must role to determine the amount of damage dealt by a blow or a fixed amount of damage done by a blow</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#definition">
        <dragonsontology:definition>Used to define entities within the ontology</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#languages -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#languages">
        <dragonsontology:definition>The languages known by characters based on their character properties</dragonsontology:definition>
        <dragonsontology:definition>The languages spoken, written, and read by a character based on character properties (race and possibly background).</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#license -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#license">
        <dragonsontology:definition>A statement pertaining to the license of this ontology</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#primaryAbility -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#primaryAbility">
        <dragonsontology:definition>The most beneficial ability for a particular character class. Abilities include Constitution, Strength, Dexterity, Wisdom, Intelligence, and Charisma.</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#size -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#size">
        <dragonsontology:definition>The size category into which a character belongs. Categories include Small and Medium for character sizes.</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#source -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#source">
        <dragonsontology:definition>Source information in the form of an ISBN or website</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#speed -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#speed">
        <dragonsontology:definition>Character&apos;s walking speed</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#weight -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/user/ontologies/2019/1/dungeons&amp;dragonsontology#weight">
        <dragonsontology:definition>The weight of an item in pounds (lb)</dragonsontology:definition>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
